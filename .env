COMPOSE_FILE=compose-files/docker-compose.yml

BOOTSTRAP_SERVERS=mm2-kafka
SECURITY_PROTOCOL=SASL_SSL
SASL_MECHANISM=SCRAM-SHA-512
SSL_TRUSTSTORE_LOCATION=/tmp/kafka.client.truststore.jks

SASL_JAAS_CONFIG=config

CONNECT_REST_ADVERTISED_HOST_NAME=mm2-kafka-connect
CONNECT_TARGET_PORT=8083
CONNECT_PUBLISHED_PORT=8083

CONNECT_GROUP_ID=mm2_connect

CONNECT_STORAGE_TOPIC_PREFIX=_mm2_connect-
CONNECT_CONFIG_STORAGE_TOPIC=connect-config
CONNECT_OFFSET_STORAGE_TOPIC=connect-offset
CONNECT_STATUS_STORAGE_TOPIC=connect-status
CONNECT_CONFIG_STORAGE_REPLICATION_FACTOR=2
CONNECT_OFFSET_STORAGE_REPLICATION_FACTOR=2
CONNECT_STATUS_STORAGE_REPLICATION_FACTOR=2

