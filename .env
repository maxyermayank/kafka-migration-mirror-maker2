CONNECT_PRODUCER_SECURITY_PROTOCOL=SASL_PLAINTEXT
CONNECT_PRODUCER_SASL_MECHANISM=PLAIN

CONNECT_KEY_CONVERTER=org.apache.kafka.connect.storage.StringConverter
CONNECT_VALUE_CONVERTER=io.confluent.connect.avro.AvroConverter

CONNECT_DEPLOY_REPLICAS=1
CONNECT_DEPLOY_NODE_GROUP=workers

KAFKA_REST_PROXY_TARGET_PORT=8082
KAFKA_REST_PROXY_PUBLISHED_PORT=8082
KAFKA_REST_PROXY_TARGET_JMX_PORT=31005
KAFKA_REST_PROXY_PUBLISHED_JMX_PORT=31005
